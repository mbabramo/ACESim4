<ACESim>
  <defineTemplate name="BargainingModuleRowsForMainReportBase">
    <rowOrColInfo name="Settlement reached">
      <filter varType="bool">
        <variableName>SettlementExists</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <!--<rowOrColInfo name="1st round settlement">
      <filter varType="bool">
        <variableName>SettlementExistsAfterFirstBargainingRound</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>-->
    <rowOrColInfoGenerator name="BargainingRound" filterVariableName="BargainingRound" rowOrColNamePrefix="Settled in bargaining round ">
      <filter varType="bool">
        <variableName>SettlementExists</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
      <filter varType="bool">
        <variableName>Beginning.PDropsCase</variableName>
        <operation>EQ</operation>
        <value>false</value>
      </filter>
      <filter varType="bool">
        <variableName>Beginning.DDefaultsCase</variableName>
        <operation>EQ</operation>
        <value>false</value>
      </filter>
      <filter varType="bool">
        <variableName>Mid.PDropsCase</variableName>
        <operation>EQ</operation>
        <value>false</value>
      </filter>
      <filter varType="bool">
        <variableName>Mid.DDefaultsCase</variableName>
        <operation>EQ</operation>
        <value>false</value>
      </filter>
      <filter varType="bool">
        <variableName>End.PDropsCase</variableName>
        <operation>EQ</operation>
        <value>false</value>
      </filter>
      <filter varType="bool">
        <variableName>End.DDefaultsCase</variableName>
        <operation>EQ</operation>
        <value>false</value>
      </filter>
    </rowOrColInfoGenerator>
    <rowOrColInfo name="Settlement failed in round 1" >
      <filter varType="int">
        <variableName>SettlementFailedAfterBargainingRoundNum</variableName>
        <operation>GTEQ</operation>
        <value>1</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="Settlement failed in round 2" >
      <filter varType="int">
        <variableName>SettlementFailedAfterBargainingRoundNum</variableName>
        <operation>GTEQ</operation>
        <value>2</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="Settlement failed in round 3" >
      <filter varType="int">
        <variableName>SettlementFailedAfterBargainingRoundNum</variableName>
        <operation>GTEQ</operation>
        <value>3</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="Settlement failed in round 4" >
      <filter varType="int">
        <variableName>SettlementFailedAfterBargainingRoundNum</variableName>
        <operation>GTEQ</operation>
        <value>4</value>
      </filter>
    </rowOrColInfo>

  </defineTemplate>
