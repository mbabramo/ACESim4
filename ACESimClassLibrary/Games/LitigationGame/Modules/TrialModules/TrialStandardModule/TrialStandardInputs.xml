<ACESim>
  <defineTemplate name="TrialModuleInputs">
    <setting name="TrialModuleInputs" type="class" subclass="TrialStandardInputs" >
      <setting name="PlaintiffMustPayDamagesIfItLoses" type="bool">$PlaintiffMustPayDamagesIfItLoses</setting>
      <setting name="DisputeResolutionRandomSeedLiability" type="distribution" subtype="uniform">
        <setting type="double" name="fromVal">0.0</setting>
        <setting type="double" name="toVal">1.0</setting>
      </setting>
      <setting name="DisputeResolutionRandomSeedDamages" type="distribution" subtype="uniform">
        <setting type="double" name="fromVal">0.0</setting>
        <setting type="double" name="toVal">1.0</setting>
      </setting>
      <setting name="JudgeNoiseLevelLiability" type="double">$JudgeNoiseLevelLiability</setting>
      <setting name="JudgeNoiseLevelDamages" type="double">$JudgeNoiseLevelDamages</setting>
      <setting name="UseProportionalResultsWhenEvolvingEarlierDecisions" type="bool">$UseProportionalResultsWhenEvolvingEarlierDecisions</setting> <!--not yet implemented for fee shifting-->
      <setting name="CaseResolvedBasedOnExogenouslySpecifiedProbability" type="bool">$CaseResolvedBasedOnExogenouslySpecifiedProbability</setting>
    </setting>
  </defineTemplate>
</ACESim>