
<ACESim>
  <commandSetsAll>
    <commandSet name="EvolveCurveFittingStrategies">
      <multipartCommand name="EvolveCurveFittingStrategies" gameInputsName="CurveFittingGameInputs">
        <!--InitializeStrategiesFile="PreevolvedStrategy"-->
        <evolveCommand
                    evolutionSettingsName="SimpleEvolution"
                    storeStrategiesFile="CurveFittingStrategies"
                    initializeStrategiesFile="CurveFittingStrategies"
                    useInitializeStrategiesFile="false"
                    evolveOnlyIfNecessary="false" />
        <playCommand numberIterations="100" StoreStrategiesFile="CurveFittingStrategies" storeIterationResultsFile="CurveFittingIterations" parallelReporting="true" savePlayedGamesInMemoryOnly="false">
          <changeSimulationSettings>
            <!--<setting name="Obfuscation" type="double">
                0.0
            </setting> DEBUG  this does not work right now; see note in InputVariables.GetSettings(). It's not passing the inputs to the variable as needed. In most cases, we shouldn't need this feature anyway. -->
          </changeSimulationSettings>
        </playCommand>
        <reportCommand name="CurveFittingReportCommand" storeIterationResultsFile="CurveFittingIterations" storeReportResultsFile="CurveFittingReport" requireCumulativeDistributions="false">
          <rowsGroup name="CurveFittingRows"></rowsGroup>
          <colsGroup name="CurveFittingColumns"></colsGroup>
        </reportCommand>

      </multipartCommand>
    </commandSet>
  </commandSetsAll>

  <executeCommandList>
    <executeCommandSet name="EvolveCurveFittingStrategies"/>
  </executeCommandList>
</ACESim>