
<ACESim>
  <commandSetsAll>
    <commandSet name="EvolvePointFittingStrategies">
      <multipartCommand name="EvolvePointFittingStrategies" gameInputsName="PointFittingGameInputs">
        <evolveCommand
                    evolutionSettingsName="SimpleEvolution"
                    storeStrategiesFile="PointFittingStrategies"
                    initializeStrategiesFile="PointFittingStrategies"
                    useInitializeStrategiesFile="false"
                    evolveOnlyIfNecessary="false" />
        <playCommand numberIterations="100" StoreStrategiesFile="PointFittingStrategies" storeIterationResultsFile="PointFittingIterations" parallelReporting="true" savePlayedGamesInMemoryOnly="false">
          <changeSimulationSettings>
            <setting name="Obfuscations" type="list">
              <setting name="Obfuscation 1" type="double">
                0.0
              </setting>
              <setting name="Obfuscation 2" type="double">
                0.0
              </setting>
              <setting name="Obfuscation 3" type="double">
                0.0
              </setting>
              <setting name="Obfuscation 4" type="double">
                0.0
              </setting>
              <setting name="Obfuscation 5" type="double">
                0.0
              </setting>
            </setting>
          </changeSimulationSettings>
        </playCommand>
        <reportCommand name="PointFittingReportCommand" storeIterationResultsFile="PointFittingIterations" storeReportResultsFile="PointFittingReport" requireCumulativeDistributions="false">
          <rowsGroup name="pointFittingRows"></rowsGroup>
          <colsGroup name="pointFittingColumns"></colsGroup>
        </reportCommand>

      </multipartCommand>
    </commandSet>
  </commandSetsAll>

  <executeCommandList>
    <executeCommandSet name="EvolvePointFittingStrategies"/>
  </executeCommandList>
</ACESim>