<ACESim>
  <parentSettingsFile filename="../DisputeGeneratorReport.xml"/>
  <defineTemplate name="DisputeGeneratorModuleRowsForMainReport">
    <insertTemplate name="DisputeGeneratorModuleRowsForMainReportBase" />
    <rowOrColInfoGenerator name="PrecautionLevelThatProducesHalfOfGainsOfInfinitePrecaution" filterVariableName="PrecautionLevelThatProducesHalfOfGainsOfInfinitePrecaution" rowOrColNamePrefix="ExpensivenessOfRiskReduction" numDynamicRanges="4">
  </rowOrColInfoGenerator>
    <rowOrColInfo name="High C/B ratio">
      <filter varType="double">
        <variableName>MarginalCostMarginalBenefitRatio</variableName>
        <operation>GT</operation>
        <value>2.0</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="Low C/B ratio">
      <filter varType="double">
        <variableName>MarginalCostMarginalBenefitRatio</variableName>
        <operation>LT</operation>
        <value>0.3</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="MC GT MB">
      <filter varType="double">
        <variableName>MarginalCostMarginalBenefitRatio</variableName>
        <operation>GT</operation>
        <value>1.0</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="MC LT MB">
      <filter varType="double">
        <variableName>MarginalCostMarginalBenefitRatio</variableName>
        <operation>LT</operation>
        <value>1.0</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="Small injuries">
      <filter varType="double">
        <variableName>MagnitudeOfInjury</variableName>
        <operation>LT</operation>
        <value>100.0</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="Large injuries">
      <filter varType="double">
        <variableName>MagnitudeOfInjury</variableName>
        <operation>GT</operation>
        <value>1900.0</value>
      </filter>
    </rowOrColInfo>
  </defineTemplate>
  <defineTemplate name="DisputeGeneratorModuleColumnsForMainReport">
    <insertTemplate name="DisputeGeneratorModuleColumnsForMainReportBase" />
    <rowOrColInfo name="MC GT MB">
      <filter varType="double">
        <variableName>MarginalCostMarginalBenefitRatio</variableName>
        <operation>GT</operation>
        <value>1.0</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="MC LT MB">
      <filter varType="double">
        <variableName>MarginalCostMarginalBenefitRatio</variableName>
        <operation>LT</operation>
        <value>1.0</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="MarginalCostMarginalBenefitRatioMedian" variableName="MarginalCostMarginalBenefitRatio" statistic="MEDIAN">
    </rowOrColInfo>
    <rowOrColInfo name="LevelOfPrecaution" variableName="LevelOfPrecaution" statistic="MEAN">
    </rowOrColInfo>
    <rowOrColInfo name="ProbabilityOfInjury" variableName="ProbabilityOfInjury" statistic="MEAN">
    </rowOrColInfo>
  </defineTemplate>
</ACESim>