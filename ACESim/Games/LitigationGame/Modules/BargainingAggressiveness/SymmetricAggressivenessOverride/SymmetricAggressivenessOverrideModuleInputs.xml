<?xml version="1.0" encoding="utf-8" ?>
<ACESim>
  <defineTemplate name="AggressivenessModuleInputs$AggressModNumber">
    <setting name="AggressivenessModuleInputs" type="class" subclass="SymmetricAggressivenessOverrideModuleInputs" >
      <setting name="MisestimateOpponentsAggressivenessWhenOptimizing" type="bool">$MisestimateOpponentsAggressivenessWhenOptimizing</setting>
      <setting name="AssumedAggressivenessOfOpponent" type="double">$AssumedAggressivenessOfOpponent</setting>
      <setting name="WeightOnAggressivenessAssumption" type="double">$WeightOnAggressivenessAssumption</setting>
      <setting name="RandomlyDeterminedStrategy" type="distribution" subtype="uniform">
        <setting name="fromVal" type="double">0.0</setting>
        <setting name="toVal" type="double">1.0</setting>
      </setting>
      <setting name="RandSeedToDetermineWhoStrategyIsFor" type="distribution" subtype="uniform">
        <setting name="fromVal" type="double">0.0</setting>
        <setting name="toVal" type="double">1.0</setting>
      </setting>
    </setting>
  </defineTemplate>
</ACESim>