<?xml version="1.0" encoding="utf-8" ?>
<ACESim>
  <defineTemplate name="AggressivenessModuleInputs$AggressModNumber">
    <setting name="AggressivenessModuleInputs" type="class" subclass="PresetAggressivenessOverrideModuleInputs" >
      <setting name="MisestimateOpponentsAggressivenessWhenOptimizing" type="bool">$MisestimateOpponentsAggressivenessWhenOptimizing</setting>
      <setting name="AssumedAggressivenessOfOpponent" type="double">$AssumedAggressivenessOfOpponent</setting>
      <setting name="WeightOnAggressivenessAssumption" type="double">$WeightOnAggressivenessAssumption</setting>
      <setting name="PPresetAggressiveness" type="distribution" subtype="uniform">
        <setting name="fromVal" type="double">$PAggressivenessOverrideModifiedMin</setting>
        <setting name="toVal" type="double">$PAggressivenessOverrideModifiedMax</setting>
      </setting>
      <setting name="DPresetAggressiveness" type="distribution" subtype="uniform">
        <setting name="fromVal" type="double">$DAggressivenessOverrideModifiedMin</setting>
        <setting name="toVal" type="double">$DAggressivenessOverrideModifiedMax</setting>
      </setting>
    </setting>
  </defineTemplate>
</ACESim>