<ACESim>
<rowOrColGroup name="PatentDamagesRows">
    <rowOrColInfo name="All">
    </rowOrColInfo>
    <rowOrColInfo name="FirstTries">
      <filter varType="bool">
        <variableName>MainInventorTries</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="SomeoneTries">
      <filter varType="bool">
        <variableName>SomeoneTries</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="NooneTries">
      <filter varType="bool">
        <variableName>SomeoneTries</variableName>
        <operation>EQ</operation>
        <value>false</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="InventionOccurs">
      <filter varType="bool">
        <variableName>InventionOccurs</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="InadvertentInfringement">
      <filter varType="bool">
        <variableName>InadvertentInfringement</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="AgreementReached">
      <filter varType="bool">
        <variableName>AgreementReached</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="IntentionalInfringement">
      <filter varType="bool">
        <variableName>IntentionalInfringement</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="InventionUsed">
      <filter varType="bool">
        <variableName>InventionUsed</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
  </rowOrColGroup>
  <rowOrColGroup name="PatentDamagesColumns">
    <rowOrColInfo name="Tot" statistic="COUNT" />
    <rowOrColInfo name="Ent" variableName="NumberEntrants" statistic="MEAN" />
    <rowOrColInfo name="Try" variableName="NumberTrying" statistic="MEAN" />
    <rowOrColInfo name="Spe" variableName="AverageSpendingOfEntrants" statistic="MEAN" />
    <rowOrColInfo name="Pay" variableName="AmountPaid" statistic="MEAN" />
    <rowOrColInfo name="HDm" variableName="HypotheticalDamages" statistic="MEAN" />
    <rowOrColInfo name="Dam" variableName="DamagesPaid" statistic="MEAN" />
    <rowOrColInfo name="IUt" variableName="AverageInventorUtility" statistic="MEAN" />
    <rowOrColInfo name="UUt" variableName="UserUtility" statistic="MEAN" />
    <rowOrColInfo name="Soc" variableName="SocialWelfare" statistic="MEAN" />
    <rowOrColInfo name="Prv" variableName="PrivateWelfare" statistic="MEAN" />
    <rowOrColInfo name="FIW" variableName="FirstInventorWinsPatent" statistic="MEAN" />
    <rowOrColInfo name="FAE" variableName="ForecastAfterEntry" statistic="MEAN" />
    <rowOrColInfo name="FAI" variableName="ForecastAfterInvestment" statistic="MEAN" />
  </rowOrColGroup>
</ACESim>