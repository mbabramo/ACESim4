
<ACESim>
  <commandSetsAll>
    <commandSet name="EvolvePatentDamagesStrategies">
      <multipartCommand name="EvolvePatentDamagesStrategies" gameInputsName="PatentDamagesGameInputs">
        <!--InitializeStrategiesFile="PreevolvedStrategy"-->
        <evolveCommand
                    evolutionSettingsName="SimpleEvolution"
                    storeStrategiesFile="PatentDamagesStrategies"
                    initializeStrategiesFile="PatentDamagesStrategies"
                    useInitializeStrategiesFile="false"
                    evolveOnlyIfNecessary="false" />
        <playCommand numberIterations="100" StoreStrategiesFile="PatentDamagesStrategies" storeIterationResultsFile="PatentDamagesIterations" parallelReporting="true" savePlayedGamesInMemoryOnly="false">
          <changeSimulationSettings>
            <!--<setting name="Obfuscation" type="double">
                0.0
            </setting> DEBUG  this does not work right now; see note in InputVariables.GetSettings(). It's not passing the inputs to the variable as needed. In most cases, we shouldn't need this feature anyway. -->
          </changeSimulationSettings>
        </playCommand>
        <reportCommand name="PatentDamagesReportCommand" storeIterationResultsFile="PatentDamagesIterations" storeReportResultsFile="PatentDamagesReport">
          <rowsGroup name="PatentDamagesRows"></rowsGroup>
          <colsGroup name="PatentDamagesColumns"></colsGroup>
        </reportCommand>

      </multipartCommand>
    </commandSet>
  </commandSetsAll>

  <executeCommandList>
    <executeCommandSet name="EvolvePatentDamagesStrategies"/>
  </executeCommandList>
</ACESim>