<?xml version="1.0" encoding="utf-8" ?>
<ACESimMultiple>
  <singleSettingsFile filename="PatentDamagesBaselineVariableSettings.xml">
    <defineVariableAlternatives name="entry">
      <defineVariableAlternative name="dyn" star="true">
        <defineVariable findString="$MaxNumEntrants" replaceString="10"/>
        <defineVariable findString="$MaxRepetitionsNumEntrantsEvolution" replaceString="9999"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="sin" star="true">
        <defineVariable findString="$MaxNumEntrants" replaceString="1"/>
        <defineVariable findString="$MaxRepetitionsNumEntrantsEvolution" replaceString="0"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="costplus">
      <defineVariableAlternative name="cpyes" star="true">
        <defineVariable findString="$WeightOnCostPlusDamages" replaceString="1.0"/>
        <defineVariable findString="$DamagesAreLesserOfTwoApproaches" replaceString="false"/>
        <defineVariable findString="$CombineInventorsForCostPlus" replaceString="false"/>
        <defineVariable findString="$UseExpectedCostForCostPlus" replaceString="false"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="cpno" star="true">
        <defineVariable findString="$WeightOnCostPlusDamages" replaceString="0"/>
        <defineVariable findString="$DamagesAreLesserOfTwoApproaches" replaceString="false"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="cpmin" star="true">
        <defineVariable findString="$DamagesAreLesserOfTwoApproaches" replaceString="true"/>
        <defineVariable findString="$CombineInventorsForCostPlus" replaceString="false"/>
        <defineVariable findString="$UseExpectedCostForCostPlus" replaceString="false"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="cphyb">
        <defineVariable findString="$WeightOnCostPlusDamages" replaceString="0.5"/>
        <defineVariable findString="$CombineInventorsForCostPlus" replaceString="false"/>
        <defineVariable findString="$UseExpectedCostForCostPlus" replaceString="false"/>
        <defineVariable findString="$DamagesAreLesserOfTwoApproaches" replaceString="false"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="cpexp">
        <defineVariable findString="$WeightOnCostPlusDamages" replaceString="1.0"/>
        <defineVariable findString="$HighestInventionValueNoiseStdev" replaceString="0.0"/>
        <defineVariable findString="$LitigationCostsEachParty" replaceString="0.1"/>
        <defineVariable findString="$UseExpectedCostForCostPlus" replaceString="true"/>
        <defineVariable findString="$ExpectedCostIsSpecificToInventor" replaceString="false"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="combinv">
        <defineVariable findString="$WeightOnCostPlusDamages" replaceString="1.0"/>
        <defineVariable findString="$PermittedRateOfReturn" replaceString="0.1"/>
        <defineVariable findString="$CombineInventorsForCostPlus" replaceString="true"/>
        <defineVariable findString="$UseExpectedCostForCostPlus" replaceString="false"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="inadv">
      <defineVariableAlternative name="noinadv" star="true">
        <defineVariable findString="$InadvertentInfringementProbability" replaceString="0.0"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="someinadv">
        <defineVariable findString="$InadvertentInfringementProbability" replaceString="0.10"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="allinadv">
        <defineVariable findString="$InadvertentInfringementProbability" replaceString="1.0"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="dammult">
      <defineVariableAlternative name="nomult" star="true">
        <defineVariable findString="$DamagesMultiplierForIntentionalInfringement" replaceString="1.0"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="tripdam">
        <defineVariable findString="$DamagesMultiplierForIntentionalInfringement" replaceString="3.0"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="return">
      <defineVariableAlternative name="stdret" star="true">
        <defineVariable findString="$MarketRateOfReturn" replaceString="0.0"/>
        <defineVariable findString="$PermittedRateOfReturn" replaceString="0.10"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="noextra">
        <defineVariable findString="$MarketRateOfReturn" replaceString="0.05"/>
        <defineVariable findString="$PermittedRateOfReturn" replaceString="0.05"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="bigret" star="true">
        <defineVariable findString="$MarketRateOfReturn" replaceString="0.00"/>
        <defineVariable findString="$PermittedRateOfReturn" replaceString="0.25"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="sprob">
      <defineVariableAlternative name="stdp" star="true">
        <defineVariable findString="$SuccessProbabilityMinimumInvestment" replaceString="0.35"/>
        <defineVariable findString="$SuccessProbabilityDoubleInvestment" replaceString="0.45"/>
        <defineVariable findString="$SuccessProbabilityTenTimesInvestment" replaceString="0.90"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="easy">
        <defineVariable findString="$SuccessProbabilityMinimumInvestment" replaceString="0.95"/>
        <defineVariable findString="$SuccessProbabilityDoubleInvestment" replaceString="0.96"/>
        <defineVariable findString="$SuccessProbabilityTenTimesInvestment" replaceString="0.99"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="hard">
        <defineVariable findString="$SuccessProbabilityMinimumInvestment" replaceString="0.15"/>
        <defineVariable findString="$SuccessProbabilityDoubleInvestment" replaceString="0.18"/>
        <defineVariable findString="$SuccessProbabilityTenTimesInvestment" replaceString="0.30"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="inois">
      <defineVariableAlternative name="inoisst" star="true">
        <defineVariable findString="$HighestInventionValueNoiseStdev" replaceString="0.10"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="inois0">
        <defineVariable findString="$HighestInventionValueNoiseStdev" replaceString="0.0"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="inoishi">
        <defineVariable findString="$HighestInventionValueNoiseStdev" replaceString="0.20"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="cost">
      <defineVariableAlternative name="cstd" star="true">
        <defineVariable findString="$CostOfMinimumInvestmentBaselineMin" replaceString="0.00"/>
        <defineVariable findString="$CostOfMinimumInvestmentBaselineMax" replaceString="0.10"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="clow">
        <defineVariable findString="$CostOfMinimumInvestmentBaselineMin" replaceString="0.00"/>
        <defineVariable findString="$CostOfMinimumInvestmentBaselineMax" replaceString="0.05"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="succind">
      <defineVariableAlternative name="indyes" star="true">
        <defineVariable findString="$SuccessIndependence" replaceString="1.0"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="indno">
        <defineVariable findString="$SuccessIndependence" replaceString="0.0"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
    <defineVariableAlternatives name="soc">
      <defineVariableAlternative name="socno" star="true">
        <defineVariable findString="$SociallyOptimalSpending" replaceString="false"/>
        <defineVariable findString="$SpilloverMultiplier" replaceString="0.0"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="socpart" star="false">
        <defineVariable findString="$SociallyOptimalSpending" replaceString="true"/>
        <defineVariable findString="$SpilloverMultiplier" replaceString="0.0"/>
      </defineVariableAlternative>
      <defineVariableAlternative name="socyes" star="false">
        <defineVariable findString="$SociallyOptimalSpending" replaceString="true"/>
        <defineVariable findString="$SpilloverMultiplier" replaceString="1.0"/>
      </defineVariableAlternative>
    </defineVariableAlternatives>
  </singleSettingsFile>
</ACESimMultiple>