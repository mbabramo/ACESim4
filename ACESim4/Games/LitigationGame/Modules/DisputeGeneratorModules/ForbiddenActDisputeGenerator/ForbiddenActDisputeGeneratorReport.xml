<ACESim>
  <parentSettingsFile filename="../DisputeGeneratorReport.xml"/>
  <defineTemplate name="DisputeGeneratorModuleRowsForMainReport">
    <insertTemplate name="DisputeGeneratorModuleRowsForMainReportBase" />
    <rowOrColInfo name="Defendant does act">
      <filter varType="bool">
        <variableName>DoesAct</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="Defendant doesn't act">
      <filter varType="bool">
        <variableName>DoesAct</variableName>
        <operation>EQ</operation>
        <value>false</value>
      </filter>
    </rowOrColInfo>
  </defineTemplate>
  <defineTemplate name="DisputeGeneratorModuleColumnsForMainReport">
    <insertTemplate name="DisputeGeneratorModuleColumnsForMainReportBase" />
    <rowOrColInfo name="Defendant does act">
      <filter varType="bool">
        <variableName>DoesAct</variableName>
        <operation>EQ</operation>
        <value>true</value>
      </filter>
    </rowOrColInfo>
    <rowOrColInfo name="PrivateBenefitOfAct" variableName="PrivateBenefitOfAct" statistic="MEAN">
    </rowOrColInfo>
    <rowOrColInfo name="SocialCostOfAct" variableName="SocialCostOfAct" statistic="MEAN">
    </rowOrColInfo>
    <rowOrColInfo name="NetSocialCost" variableName="NetSocialCost" statistic="MEAN">
    </rowOrColInfo>
  </defineTemplate>
</ACESim>