<?xml version="1.0" encoding="utf-8" ?>

<ACESim>
    <gameInputsAll>
      <gameInputs name="BargainingGame1Inputs" gameName="BargainingGame1">
        <setting name="wastePct" type="distribution" subtype="uniform">
          <setting type="double" name="fromVal">0.0</setting>
          <setting type="double" name="toVal">1.0</setting>
        </setting>
        <setting name="player1PctOfNotWasted" type="distribution" subtype="uniform">
          <setting type="double" name="fromVal">0.0</setting>
          <setting type="double" name="toVal">1.0</setting>
        </setting>
        <setting name="obfuscationLevelForSelf" type="distribution" subtype="uniform">
          <setting type="double" name="fromVal">0.0</setting>
          <setting type="double" name="toVal">0.2</setting>
        </setting>
        <setting name="obfuscationLevelForOpponent" type="distribution" subtype="uniform">
          <setting type="double" name="fromVal">0.0</setting>
          <setting type="double" name="toVal">0.2</setting>
        </setting>
        <setting name="obfuscationRealizedOfPlayer1ResultForPlayer1" type="distribution" subtype="normal">
          <setting type="double" name="mean">0.0</setting>
          <setting type="variableFromSetting" name="stdev">obfuscationLevelForSelf</setting>
        </setting>
        <setting name="obfuscationRealizedOfPlayer1ResultForPlayer2" type="distribution" subtype="normal">
          <setting type="double" name="mean">0.0</setting>
          <setting type="variableFromSetting" name="stdev">obfuscationLevelForOpponent</setting>
        </setting>
        <setting name="obfuscationRealizedOfPlayer2ResultForPlayer1" type="distribution" subtype="normal">
          <setting type="double" name="mean">0.0</setting>
          <setting type="variableFromSetting" name="stdev">obfuscationLevelForOpponent</setting>
        </setting>
        <setting name="obfuscationRealizedOfPlayer2ResultForPlayer2" type="distribution" subtype="normal">
          <setting type="double" name="mean">0.0</setting>
          <setting type="variableFromSetting" name="stdev">obfuscationLevelForSelf</setting>
        </setting>
        <!--<setting name="deobfuscator" type="strategy" filename="../../ObfuscationGame/Strategies/ObfuscationGameStrategies" decisionNum="1">
        </setting>-->
      </gameInputs>
    </gameInputsAll>
    
</ACESim>